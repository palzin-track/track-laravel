{
    "name": "palzin-track/track-laravel",
    "description": "Get a realtime feed of your Laravel projectâ€™s most important events using Palzin Track",
    "keywords": [
        "Palzin",
        "laravel",
        "laravel-Track"
    ],
    "homepage": "https://github.com/palzin-track/track-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Palzin Team",
            "email": "hello@palzin.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Palzin\\Track\\": "src",
            "Palzin\\Track\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Palzin\\Track\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Palzin\\Track\\TrackServiceProvider"
            ],
            "aliases": {
                "Track": "Palzin\\Track\\Facades\\Track"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}